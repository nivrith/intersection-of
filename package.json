{
  "name": "intersection-of",
  "version": "1.2.4",
  "description": "Find unique common elements among given arrays",
  "keywords": [
    "array",
    "array-intersection",
    "common",
    "common-array",
    "elements",
    "fast",
    "intersection",
    "typescript",
    "unique"
  ],
  "repository": "https://github.com/nivrith/intersection-of",
  "license": "MIT",
  "author": "Nivrith <nivrithgomatam@gmail.com>",
  "files": [
    "dist/src"
  ],
  "main": "dist/src",
  "scripts": {
    "clean": "del-cli dist",
    "compile": "npm run clean && tsc -b",
    "docs": "typedoc src",
    "prepublishOnly": "npm run compile",
    "start": "yarn compile && node dist/src/index",
    "pretest": "npm run compile",
    "test": "nyc mocha --require ts-node/register test/**/*.spec.ts",
    "release": "npx np",
    "benchmark": "ts-node test/benchmark",
    "version": "npx auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1",
    "fast-memoize": "^2.5.1",
    "lodash.intersection": "^4.4.0",
    "lodash.intersectionby": "^4.7.0",
    "lodash.intersectionwith": "^4.4.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.134",
    "@types/lodash.intersection": "^4.4.6",
    "@types/lodash.intersectionby": "^4.7.6",
    "@types/lodash.intersectionwith": "^4.4.6",
    "@types/mocha": "^5.2.7",
    "auto-changelog": "^1.13.0",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "del-cli": "^2.0.0",
    "mocha": "^6.1.4",
    "np": "^5.0.3",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "lines": 80,
    "exclude": [
      "test",
      "docs",
      "dist"
    ],
    "check-coverage": true
  }
}
